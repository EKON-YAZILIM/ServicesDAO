@{
    Layout = "~/Views/Shared/_MainLayout.cshtml";
}

<div>
    <h2>My Available Reputation:  0.00000</h2>
    <p>Total: 0.00000</p>
    <p>Staked: 0.00000</p>
    <p>Minted Pending:0.00000</p>
</div>
<div class="tab-content rounded-bottom mt-2">
    <div class="tab-pane p-3 active" role="tabpanel" id="preview-924">
        <div class="card m-2">
            <div class="card-header">
                <span class="small ms-1">Reputation History Table</span>
            </div>
            <div class="card-body">
                <table class="table table-striped table-hover">
                    <thead>
                        <tr>
                            <th scope="col">#</th>
                            <th scope="col">Username</th>
                            <th scope="col">Price</th>
                            <th scope="col">Time</th>
                            <th scope="col">Reputation Stake</th>
                        </tr>
                    </thead>
                    <tbody>
                        @{int Count = 0;}
                        <tr>
                            <th scope="row">@Count</th>
                            <td>item.UserName</td>
                            <td>item.Price</td>
                            <td>item.Time</td>
                            <td>item.ReputationStake</td>
                        </tr>
                    </tbody>
                </table>
            </div>
        </div>
    </div>
</div>
